{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm install && concurrently \"npm run client:start\" \"npm run server:start\"",
    "client:start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "server:start": "cd server/nest-app && npm install && npm run start:dev",
    "server:install": "cd server/nest-app & npm install"
  },
  "dependencies": {
    "@quasar/extras": "^1.14.2",
    "axios": "^0.27.2",
    "concurrently": "^7.2.2",
    "pinia": "^2.0.14",
    "quasar": "^2.7.5",
    "vue": "^3.2.37",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.0.10",
    "@vitejs/plugin-vue": "^2.3.3",
    "sass": "1.32.12",
    "typescript": "^4.6.4",
    "vite": "^2.9.13",
    "vue-tsc": "^0.38.2"
  }
}
